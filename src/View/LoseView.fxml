      </Label>
      <ScrollPane layoutX="145.0" layoutY="164.0" prefHeight="367.0" prefWidth="456.0">
        <content>
          <AnchorPane>
               <children>
                   <Label fx:id="high_scores_table" alignment="TOP_LEFT" contentDisplay="TOP" lineSpacing="0.5" wrapText="true">
                       <font>
                           <Font name="Impact" size="25.0" />
                       </font>
                   </Label>
               </children>
            </AnchorPane>
        </content>
